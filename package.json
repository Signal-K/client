{
  "name": "v2",
  "version": "0.1.0",
  "private": true,
  "dependencyPolicy": {
    "protected": [
      "d3",
      "markerjs2",
      "posthog-js",
      "posthog-node"
    ]
  },
  "resolutions": {
    "camera-controls": "2.9.0"
  },
  "scripts": {
    "flask-dev": "FLASK_DEBUG=1 pip3 install -r requirements.txt && python3 -m flask --app api/index run -p 5328",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --quiet",
    "knip": "knip",
    "lock:check": "node scripts/check-lockfile-sync.cjs",
    "bundle:check": "node scripts/check-bundle-budget.cjs",
    "notify:discoveries": "node scripts/notify-unclassified-discoveries.js",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "env -u ELECTRON_RUN_AS_NODE cypress run",
    "test:e2e:local": "SKIP_USER_CREATION_TESTS=false env -u ELECTRON_RUN_AS_NODE cypress run",
    "test:e2e:open": "env -u ELECTRON_RUN_AS_NODE cypress open",
    "test:e2e:smoke:minimal": "SKIP_USER_CREATION_TESTS=true env -u ELECTRON_RUN_AS_NODE cypress run --headless --spec cypress/e2e/smoke-critical.cy.ts",
    "test:e2e:smoke:full": "SKIP_USER_CREATION_TESTS=true env -u ELECTRON_RUN_AS_NODE cypress run --headless --spec cypress/e2e/smoke-critical.cy.ts,cypress/e2e/contributions-critical.cy.ts",
    "test:e2e:smoke": "npm run test:e2e:smoke:full",
    "test:e2e:headless": "SKIP_USER_CREATION_TESTS=true env -u ELECTRON_RUN_AS_NODE cypress run --headless",
    "test:suite:local": "scripts/tests/run-local-suite.sh",
    "test:all": "npm run test:unit && npm run test:e2e",
    "docker:test:unit": "docker-compose -f ops/compose/docker-compose.test.yml --profile unit up --build --abort-on-container-exit",
    "docker:test:e2e": "docker-compose -f ops/compose/docker-compose.test.yml --profile e2e up --build --abort-on-container-exit",
    "docker:test:e2e:local": "docker-compose -f ops/compose/docker-compose.test.yml --profile local-test up --build --abort-on-container-exit",
    "docker:test:all": "docker-compose -f ops/compose/docker-compose.test.yml --profile all up --build --abort-on-container-exit",
    "docker:test:clean": "docker-compose -f ops/compose/docker-compose.test.yml down --volumes --remove-orphans",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "ssr:check": "scripts/check-ssr-data-boundaries.sh"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@prisma/client": "^6.19.2",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-progress": "1.1.0",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "1.1.3",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.14",
    "@react-three/drei": "^9.121.5",
    "@react-three/fiber": "^8.17.14",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.42.7",
    "@types/node": "^22.7.8",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "antd": "^5.19.3",
    "antd-style": "^3.6.2",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.0.5",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.394.0",
    "next": "^14.2.21",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.30",
    "posthog-js": "^1.298.1",
    "posthog-node": "^5.14.1",
    "prisma": "^6.19.2",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-layout-kit": "^1.9.0",
    "react-webcam": "^7.2.0",
    "sass": "^1.77.4",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.173.0",
    "typescript": "^5.6.3",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.7.8",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/web-push": "^3.6.4",
    "@vitest/coverage-v8": "3.2.4",
    "cypress": "^14.5.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.21",
    "jsdom": "^26.1.0",
    "knip": "^5.33.3",
    "markerjs2": "^2.32.3",
    "postcss": "^8.4.30",
    "postcss-load-config": "^6.0.1",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=19.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
