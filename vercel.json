{
    "version": 1,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "app.py"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}